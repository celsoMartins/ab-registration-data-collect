<h1>Lista de inscritos</h1>
<table class="table table-hover table-condensed">
  <thead>
      <tr>
        <th>#</th>
        <th>Data do Report</th>
        <th>Iniciadas</th>
        <th>Abertas</th>
        <th>Disponíveis</th>
        <th>Em Análise</th>
        <th>Pagas</th>
        <th>Em Disputa</th>
        <th>Reembolsadas</th>
        <th>Canceladas</th>
        <th>Total</th>
      </tr>
  </thead>

  <% count = 1 %>
  <% @transactions.each do |transaction|%>
      <tbody>
          <tr>
            <td><%= count %></td>
            <td><%= l transaction.date, format: :long %></td>
            <td><%= transaction.qtd_initiated %></td>
            <td><%= transaction.qtd_open %></td>
            <td><%= transaction.qtd_available %></td>
            <td><%= transaction.qtd_in_analysis %></td>
            <td><%= transaction.qtd_paid %></td>
            <td><%= transaction.qtd_in_dispute %></td>
            <td><%= transaction.qtd_refunded %></td>
            <td><%= transaction.qtd_cancelled %></td>
            <td><%= transaction.qtd_total %></td>
            <% count += 1%>
          </tr>
      </tbody>
  <% end %>
</table>


